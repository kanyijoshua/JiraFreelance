@model IEnumerable<jirafrelance.Models.TblPaymentDeduction>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PayDeductionDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PayDeductionAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkPayDeductionHistoryNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PayDeductionDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PayDeductionAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkPayDeductionHistoryNavigation.FinalPay)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkPayDeductionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PkPayDeductionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PkPayDeductionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
